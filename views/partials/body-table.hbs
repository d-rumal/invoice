<table width="100%">
    <tbody>
        <tr>
            <th class="light service_colm">SERVICE</th>
            <th class="light">RATE</th>
            <th class="light">TAX</th>
            <th class="light">QTY</th>
            <th class="light total_colm">TOTAL</th>
        </tr>

        {{#each data.orders}}
        <tr>
            <td class="service_colm">
                <span class="bold table_box">{{this.product_name}}</span>
                <span class="table_box">{{this.product_desc}}</span>
            </td>
            <td>${{this.product_rate}}</td>
            <td>{{this.product_tax}}%</td>
            <td>{{this.product_qty}}</td>
            <td class="total_colm">${{this.product_total}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>